# Supabase Configuration
# These are PUBLIC variables (safe for client-side use)
PUBLIC_SUPABASE_URL=https://your-project.supabase.co
PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Server-side Supabase Configuration (PRIVATE - server only)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key

# OAuth Configuration (PRIVATE - server only)
# Google OAuth
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=https://yourdomain.com/auth/google/callback

# Notion OAuth
NOTION_CLIENT_ID=your_notion_client_id
NOTION_CLIENT_SECRET=your_notion_client_secret
NOTION_REDIRECT_URI=https://yourdomain.com/auth/notion/callback

# MCP Integration (PRIVATE - server only)
MCP_API_KEY=your_secure_mcp_api_key

# N8N Integration (PRIVATE - server only)
N8N_DEMO_CHAT_API_URL=https://your-n8n-instance.com/webhook/your-webhook-id/chat

# Analytics (PUBLIC - client-side)
VITE_POSTHOG_API_KEY=your_posthog_api_key

# PostHog Configuration (PRIVATE - server only)
POSTHOG_KEY=your_posthog_key
POSTHOG_HOST=https://us.i.posthog.com 